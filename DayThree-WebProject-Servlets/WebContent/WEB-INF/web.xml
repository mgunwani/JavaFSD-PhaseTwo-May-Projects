<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>DayThree-WebProject-Servlets</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>Driver</param-name>
    <param-value>com.mysql.cj.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>URL</param-name>
    <param-value>http://localhost:8081/abc</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>FirstServlet</display-name>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>com.bhawnagunwani.FirstServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/FirstServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DemoOneServlet</display-name>
    <servlet-name>DemoOneServlet</servlet-name>
    <servlet-class>com.bhawnagunwani.DemoOneServlet</servlet-class>
    <init-param>
      <param-name>Message</param-name>
      <param-value>Hello from ServletConfig to DemoOneServlet</param-value>
    </init-param>
    <init-param>
      <param-name>Command</param-name>
      <param-value>SELECT * FROM Employees</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>DemoOneServlet</servlet-name>
    <url-pattern>/DemoOneServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DemoTwoServlet</display-name>
    <servlet-name>DemoTwoServlet</servlet-name>
    <servlet-class>com.bhawnagunwani.DemoTwoServlet</servlet-class>
    <init-param>
      <param-name>Message</param-name>
      <param-value>Hello from ServletConfig to DemoTwoServlet</param-value>
    </init-param>
    <init-param>
      <param-name>Command</param-name>
      <param-value>SELECT * FROM Departments</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>DemoTwoServlet</servlet-name>
    <url-pattern>/DemoTwoServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.bhawnagunwani.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>WelcomeServlet</display-name>
    <servlet-name>WelcomeServlet</servlet-name>
    <servlet-class>com.bhawnagunwani.WelcomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>WelcomeServlet</servlet-name>
    <url-pattern>/WelcomeServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RegisterServlet</display-name>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.bhawnagunwani.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>ValidationFilter</display-name>
    <filter-name>ValidationFilter</filter-name>
    <filter-class>com.bhawnagunwani.filters.ValidationFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ValidationFilter</filter-name>
    <servlet-name>RegisterServlet</servlet-name>
  </filter-mapping>
</web-app>